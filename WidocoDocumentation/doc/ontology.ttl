@prefix : <http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/> .

<http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/> rdf:type owl:Ontology ;
                                                                          rdfs:comment "Descripción: Este modelo ontológico busca ser un mapa detallado que guía la comprensión de las interconexiones dentro de la industria turística ecuatoriana" ;
                                                                          owl:versionInfo 1.0 .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/compran
:compran rdf:type owl:ObjectProperty ;
         owl:inverseOf :esvendidoA .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/destino
:destino rdf:type owl:ObjectProperty ;
         rdfs:domain :PaqueteTuristico ;
         rdfs:range :DestinoTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/esvendidoA
:esvendidoA rdf:type owl:ObjectProperty ;
            rdfs:domain :PaqueteTuristico ;
            rdfs:range :Turista .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/genero
:genero rdf:type owl:ObjectProperty ;
        rdfs:domain :Turista ;
        rdfs:range :Genero .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ofrece
:ofrece rdf:type owl:ObjectProperty ;
        owl:inverseOf :ofrecidoPor .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ofreceActividad
:ofreceActividad rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :ofrece ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Alojamiento
                                             :PaqueteTuristico
                                             :Ubicacion
                                           )
                             ] .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ofreceHospedaje
:ofreceHospedaje rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :ofrece ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :PaqueteTuristico
                                             :Ubicacion
                                           )
                             ] .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ofreceTransporte
:ofreceTransporte rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :ofrece ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( :PaqueteTuristico
                                              :Ubicacion
                                            )
                              ] .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ofrecidoPor
:ofrecidoPor rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tiene
:tiene rdf:type owl:ObjectProperty ;
       owl:inverseOf :ubicadoEn .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tieneDescuento
:tieneDescuento rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :PaqueteTuristico
                                            :Turista
                                          )
                            ] ;
                rdfs:range :Descuento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ubicadoEn
:ubicadoEn rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Alojamiento
                                       :DestinoTuristico
                                       :Ubicacion
                                     )
                       ] ;
           rdfs:range :Ubicacion .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ubicadoEnCiudad
:ubicadoEnCiudad rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :ubicadoEn ;
                 rdfs:range :Ciudad .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ubicadoEnMontaña
:ubicadoEnMontaña rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :ubicadoEn ;
                  rdfs:range :Montana .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ubicadoEnPlaya
:ubicadoEnPlaya rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :ubicadoEn ;
                rdfs:range :Playa .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/vende
:vende rdf:type owl:ObjectProperty ;
       owl:inverseOf :vendidoPor ;
       rdfs:domain :AgenciaTurismo ;
       rdfs:range :PaqueteTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/vendidoPor
:vendidoPor rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/viajaCon
:viajaCon rdf:type owl:ObjectProperty ;
          owl:inverseOf :viajaCon ;
          rdf:type owl:SymmetricProperty ;
          rdfs:domain :Turista ;
          rdfs:range :Turista .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/capacidad
:capacidad rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain :Transporte ;
           rdfs:range xsd:integer .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/edad
:edad rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/precio
:precio rdf:type owl:DatatypeProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Alojamiento
                                    :PaqueteTuristico
                                  )
                    ] ;
        rdfs:range xsd:decimal .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tieneAereopuerto
:tieneAereopuerto rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf :tieneEstacion ;
                  rdfs:range xsd:boolean .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tieneEstacion
:tieneEstacion rdf:type owl:DatatypeProperty ;
               rdfs:domain :Ubicacion .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tieneEstacionTren
:tieneEstacionTren rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf :tieneEstacion ;
                   rdfs:domain :Ciudad ;
                   rdfs:range xsd:boolean .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tieneNombre
:tieneNombre rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tienePuerto
:tienePuerto rdf:type owl:DatatypeProperty ;
             rdfs:subPropertyOf :tieneEstacion ;
             rdfs:range xsd:boolean .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/tieneTerminal
:tieneTerminal rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf :tieneEstacion ;
               rdfs:range xsd:boolean .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/vendidoEnMes
:vendidoEnMes rdf:type owl:DatatypeProperty ;
              rdfs:domain :PaqueteTuristico .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Actividad
:Actividad rdf:type owl:Class ;
           rdfs:comment "Clase que describe los tipos de actividades que se pueden realizar."@es ,
                        "Class that describes the types of activities that can be performed."@en .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ActividadCultural
:ActividadCultural rdf:type owl:Class ;
                   rdfs:subClassOf :Actividad .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ActividadDeEntretenimiento
:ActividadDeEntretenimiento rdf:type owl:Class ;
                            rdfs:subClassOf :Actividad .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/ActividadDeportiva
:ActividadDeportiva rdf:type owl:Class ;
                    rdfs:subClassOf :Actividad .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/AgenciaTurismo
:AgenciaTurismo rdf:type owl:Class ;
                rdfs:comment "Tourism company class"@en ,
                             "Clase de la empresa de turismo" .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Alojamiento
:Alojamiento rdf:type owl:Class ;
             rdfs:comment "Clase con los tipos de alojamiento disponible"@es ,
                          "Class with the types of accommodation available"@en .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/AutoBus
:AutoBus rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :capacidad ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:integer ;
                                                    owl:withRestrictions ( [ xsd:minInclusive 16
                                                                           ]
                                                                           [ xsd:maxInclusive 70
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :Transporte ;
         owl:disjointWith :Avion ,
                          :Lancha ,
                          :Tren .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Avion
:Avion rdf:type owl:Class ;
       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :capacidad ;
                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                  owl:onDatatype xsd:integer ;
                                                  owl:withRestrictions ( [ xsd:minExclusive 70
                                                                         ]
                                                                         [ xsd:maxInclusive 150
                                                                         ]
                                                                       )
                                                ]
                           ] ;
       rdfs:subClassOf :Transporte ;
       owl:disjointWith :Lancha ,
                        :Tren .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Ciudad
:Ciudad rdf:type owl:Class ;
        rdfs:subClassOf :Ubicacion .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DescNinos
:DescNinos rdf:type owl:Class ;
           rdfs:subClassOf :Descuento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DescPorTemporada
:DescPorTemporada rdf:type owl:Class ;
                  rdfs:subClassOf :Descuento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DescTerceraEdad
:DescTerceraEdad rdf:type owl:Class ;
                 rdfs:subClassOf :Descuento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Descuento
:Descuento rdf:type owl:Class .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DestinoCultural
:DestinoCultural rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :ofrece ;
                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass :ActividadCultural
                                     ] ;
                 rdfs:subClassOf :DestinoTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DestinoDeEntretenimiento
:DestinoDeEntretenimiento rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty :ofrece ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass :ActividadDeEntretenimiento
                                              ] ;
                          rdfs:subClassOf :DestinoTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DestinoDeportivo
:DestinoDeportivo rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :ofrece ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :ActividadDeportiva
                                      ] ;
                  rdfs:subClassOf :DestinoTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/DestinoTuristico
:DestinoTuristico rdf:type owl:Class .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Femenino
:Femenino rdf:type owl:Class ;
          rdfs:subClassOf :Genero .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Genero
:Genero rdf:type owl:Class .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Glamping
:Glamping rdf:type owl:Class ;
          rdfs:subClassOf :Alojamiento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Hotel
:Hotel rdf:type owl:Class ;
       rdfs:subClassOf :Alojamiento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/HotelDeLujo
:HotelDeLujo rdf:type owl:Class ;
             rdfs:subClassOf :Hotel .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/HotelEconomico
:HotelEconomico rdf:type owl:Class ;
                rdfs:subClassOf :Hotel .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Lancha
:Lancha rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :capacidad ;
                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:onDatatype xsd:integer ;
                                                   owl:withRestrictions ( [ xsd:maxInclusive 15
                                                                          ]
                                                                        )
                                                 ]
                            ] ;
        rdfs:subClassOf :Transporte ;
        owl:disjointWith :Tren .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Masculino
:Masculino rdf:type owl:Class ;
           rdfs:subClassOf :Genero .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Montana
:Montana rdf:type owl:Class ;
         rdfs:subClassOf :Ubicacion .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/PaqueteCultural
:PaqueteCultural rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :destino ;
                                       owl:someValuesFrom :DestinoCultural
                                     ] ;
                 rdfs:subClassOf :PaqueteTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/PaqueteDeEntretenimiento
:PaqueteDeEntretenimiento rdf:type owl:Class ;
                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                owl:onProperty :destino ;
                                                owl:someValuesFrom :DestinoDeEntretenimiento
                                              ] ;
                          rdfs:subClassOf :PaqueteTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/PaqueteDeportivo
:PaqueteDeportivo rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :destino ;
                                        owl:someValuesFrom :DestinoDeportivo
                                      ] ;
                  rdfs:subClassOf :PaqueteTuristico .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/PaqueteTuristico
:PaqueteTuristico rdf:type owl:Class .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Playa
:Playa rdf:type owl:Class ;
       rdfs:subClassOf :Ubicacion .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Resort
:Resort rdf:type owl:Class ;
        rdfs:subClassOf :Alojamiento .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Transporte
:Transporte rdf:type owl:Class .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Tren
:Tren rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :capacidad ;
                            owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                 owl:onDatatype xsd:integer ;
                                                 owl:withRestrictions ( [ xsd:minExclusive 150
                                                                        ]
                                                                        [ xsd:maxInclusive 470
                                                                        ]
                                                                      )
                                               ]
                          ] ;
      rdfs:subClassOf :Transporte .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Turista
:Turista rdf:type owl:Class .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/TuristaAdulto
:TuristaAdulto rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :edad ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:integer ;
                                                          owl:withRestrictions ( [ xsd:maxExclusive 65
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :TuristaMayorDeEdad .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/TuristaAdultoMayor
:TuristaAdultoMayor rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :edad ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:integer ;
                                                               owl:withRestrictions ( [ xsd:minInclusive 65
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf :TuristaMayorDeEdad .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/TuristaMayorDeEdad
:TuristaMayorDeEdad rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :edad ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:integer ;
                                                               owl:withRestrictions ( [ xsd:minInclusive 18
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf :Turista .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/TuristaMenorDeEdad
:TuristaMenorDeEdad rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :edad ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:integer ;
                                                               owl:withRestrictions ( [ xsd:maxInclusive 17
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf :Turista ,
                                    :TuristaAdulto .


###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/Ubicacion
:Ubicacion rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/crow/ontologies/2025/2/example.org/turismo/actividadCultural1
:actividadCultural1 rdf:type owl:NamedIndividual ,
